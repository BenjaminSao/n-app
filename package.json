{
  "name": "n-app",
  "version": "1.0.9",
  "description": "Vue.js based application framework",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "ts-build": "tsc -p .",
    "ts-build-watch": "npm run ts-build -- -w",
    "ts-build-dist": "tsc -p ./dist",
    "clean-src": "find ./src -name '*.js' -delete -o -name '*.map' -delete",
    "clean-test": "find ./test -name '*.js' -delete -o -name '*.map' -delete",
    "clean-test-app": "find ./test-app -name '*.js' -delete -o -name '*.map' -delete",
    "clean-dist": "find ./dist -name '*.js' -delete -o -name '*.map' -delete -o -name '*.ts' -delete",
    "clean-all": "npm run clean-src && npm run clean-test && npm run clean-test-app && npm run clean-dist",
    "pretest": "npm run ts-build",
    "test": "mocha --reporter spec --ui tdd --require ts-node/register './test/**/*.test.js' || true",
    "pretest-ci": "npm run pretest",
    "test-ci": "mocha --reporter spec --ui tdd --require ts-node/register './test/**/*.test.js'",
    "pregit-publish": "npm run ts-build-dist && git add . && git commit -m 'preparing to publish new version'",
    "git-publish": "npm version patch",
    "postgit-publish": "git push",
    "prebundle": "npm run ts-build",
    "bundle": "webpack ./test-app/client/app.js ./test-app/client/dist/scripts/app-bundle.js",
    "bundle-watch": "npm run bundle -- -w",
    "nodemon": "nodemon --debug ./test-app/main.js",
    "predebug": "npm run ts-build",
    "debug": "npm run ts-build-watch & npm run bundle-watch & npm run nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nivinjoseph/n-app.git"
  },
  "keywords": [
    "SPA",
    "Vue.js",
    "Framework"
  ],
  "author": "NiviN",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nivinjoseph/n-app/issues"
  },
  "homepage": "https://github.com/nivinjoseph/n-app#readme",
  "devDependencies": {
    "@types/jquery": "^2.0.42",
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.5",
    "@types/sinon": "^1.16.35",
    "@types/toastr": "^2.1.33",
    "mocha": "^3.2.0",
    "n-config": "git+https://github.com/nivinjoseph/n-config.git",
    "n-web": "git+https://github.com/nivinjoseph/n-web.git",
    "nodemon": "^1.11.0",
    "sinon": "^2.0.0-pre.6",
    "ts-node": "^2.1.0",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1",
    "vue": "^2.2.6",
    "vue-router": "^2.5.1",
    "webpack": "^2.4.1"
  },
  "dependencies": {
    "n-defensive": "git+https://github.com/nivinjoseph/n-defensive.git",
    "n-exception": "git+https://github.com/nivinjoseph/n-exception.git",
    "n-ext": "git+https://github.com/nivinjoseph/n-ext.git",
    "n-ject": "git+https://github.com/nivinjoseph/n-ject.git",
    "reflect-metadata": "^0.1.10"
  }
}
