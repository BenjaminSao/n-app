{
    "name": "@nivinjoseph/n-app",
    "version": "3.0.52",
    "description": "Vue.js based application framework",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "ts-compile": "tsc -p .",
        "ts-lint": "tslint -p . -c tslint.json",
        "ts-build": "npm run ts-compile && npm run ts-lint",
        "ts-build-dist": "npm run ts-build && tsc -p ./dist",
        "ts-build-watch": "npm run ts-compile -- -w",
        "clean-src": "find ./src -name '*.js' -delete -o -name '*.map' -delete",
        "clean-test": "find ./test -name '*.js' -delete -o -name '*.map' -delete",
        "clean-test-app": "find ./test-app -name '*.js' -delete -o -name '*.map' -delete",
        "clean-dist": "find ./dist -name '*.js' -delete -o -name '*.map' -delete -o -name '*.ts' -delete",
        "clean-all": "npm run clean-src && npm run clean-test && npm run clean-test-app && npm run clean-dist",
        "pretest": "npm run ts-build",
        "test": "mocha --reporter spec --ui tdd --require ts-node/register './test/**/*.test.js' || true",
        "pretest-ci": "npm run pretest",
        "test-ci": "mocha --reporter spec --ui tdd --require ts-node/register './test/**/*.test.js'",
        "publish-package": "npm run ts-build-dist && git add . && git commit -m 'preparing to publish new version' && npm version patch && git push && npm publish --access=public",
        "prebundle": "npm run ts-build",
        "webpack-build": "webpack",
        "webpack-build-watch": "npm run webpack-build -- -w",
        "nodemon": "nodemon --ignore src/ --ignore test/ --ignore test-app/client/ --inspect=4001 ./test-app/main.js env=dev",
        "predev": "npm run ts-build",
        "x-dev": "npm run ts-build-watch & npm run nodemon",
        "dev": "concurrently \"npm:ts-build-watch\" \"npm:nodemon\"",
        "build": "npm run ts-build && env=prod npm run webpack-build",
        "start": "node ./test-app/main.js env=prod"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nivinjoseph/n-app.git"
    },
    "keywords": [
        "SPA",
        "Vue.js",
        "Framework"
    ],
    "author": "NiviN",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/nivinjoseph/n-app/issues"
    },
    "homepage": "https://github.com/nivinjoseph/n-app#readme",
    "devDependencies": {
        "@nivinjoseph/n-web": "^3.2.57",
        "@types/mocha": "^8.2.2",
        "@types/node": "^14.14.43",
        "@types/sharp": "^0.28.0",
        "@types/toastr": "^2.1.38",
        "concurrently": "^6.0.2",
        "feather-icons": "^4.28.0",
        "mocha": "^8.3.2",
        "nodemon": "^2.0.7",
        "ts-node": "^9.1.1",
        "tslint": "^6.1.3",
        "typescript": "^4.2.4",
        "typescript-tslint-plugin": "^0.5.5"
    },
    "dependencies": {
        "@babel/core": "^7.14.0",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.14.0",
        "@nivinjoseph/n-config": "^1.0.32",
        "@nivinjoseph/n-defensive": "^1.0.34",
        "@nivinjoseph/n-exception": "^1.0.20",
        "@nivinjoseph/n-ext": "^1.1.23",
        "@nivinjoseph/n-ject": "^1.1.13",
        "@nivinjoseph/n-util": "^1.0.69",
        "@nivinjoseph/vue": "^2.6.11",
        "@types/jquery": "^3.5.5",
        "autoprefixer": "^10.2.5",
        "babel-loader": "^8.2.2",
        "btoa": "^1.2.1",
        "clean-webpack-plugin": "^3.0.0",
        "compression-webpack-plugin": "^6.1.1",
        "css-loader": "^5.2.4",
        "enhanced-resolve": "^5.8.0",
        "fibers": "^5.0.0",
        "file-loader": "^6.2.0",
        "fork-ts-checker-webpack-plugin": "^6.2.5",
        "hash-sum": "^2.0.0",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^4.5.1",
        "imagemin": "^7.0.1",
        "imagemin-gifsicle": "^7.0.0",
        "imagemin-jpegtran": "^7.0.0",
        "imagemin-mozjpeg": "^9.0.0",
        "imagemin-pngquant": "^9.0.2",
        "imagemin-svgo": "^9.0.0",
        "imagemin-webp": "^6.0.0",
        "jquery": "^3.6.0",
        "loader-utils": "^2.0.0",
        "mini-css-extract-plugin": "^1.3.9",
        "node-sass": "^4.14.1",
        "optimize-css-assets-webpack-plugin": "^5.0.4",
        "postcss": "^8.2.13",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-loader": "^4.2.0",
        "reflect-metadata": "^0.1.13",
        "sass": "^1.32.11",
        "sass-loader": "^10.1.1",
        "sharp": "^0.28.1",
        "simplebar": "^5.3.2",
        "source-map-loader": "^1.1.3",
        "source-map-support": "^0.5.19",
        "style-loader": "^2.0.0",
        "terser-webpack-plugin": "^4.2.3",
        "ts-loader": "^8.1.0",
        "tslib": "^2.2.0",
        "tslint-loader": "^3.5.4",
        "url-loader": "^4.1.1",
        "vue-hot-reload-api": "^2.3.4",
        "vue-loader": "^15.9.6",
        "vue-router": "3.1.6",
        "vue-template-compiler": "2.6.11",
        "webpack": "^4.44.2",
        "webpack-cli": "^4.1.0",
        "worker-loader": "^3.0.8"
    },
    "browserslist": [
        "defaults"
    ]
}
